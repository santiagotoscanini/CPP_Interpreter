module SkelCPP where

-- Haskell module generated by the BNF converter

import AbsCPP
import ErrM
type Result = Err String

failure :: Show a => a -> Result
failure x = Bad $ "Undefined case: " ++ show x

transId :: Id -> Result
transId x = case x of
  Id string -> failure x
transProgram :: Program -> Result
transProgram x = case x of
  PDefs defs -> failure x
transDef :: Def -> Result
transDef x = case x of
  DFun type_ id args stms -> failure x
  DFunSBdy type_ id args -> failure x
  DFunInline type_ id args stms -> failure x
  DFunSBdyIL type_ id args -> failure x
  DUs qconst -> failure x
  DType type_ id -> failure x
  DDecls decl -> failure x
  DStruct id decls -> failure x
transDecl :: Decl -> Result
transDecl x = case x of
  Decl type_ ids -> failure x
  DInit type_ id exp -> failure x
transArg :: Arg -> Result
transArg x = case x of
  ADeclBlnck type_ -> failure x
  ADecl type_ id -> failure x
  ADeclIn type_ id exp -> failure x
  AConst arg -> failure x
transStm :: Stm -> Result
transStm x = case x of
  SEmpty -> failure x
  SExp exp -> failure x
  SDecls decl -> failure x
  SReturn exp -> failure x
  SReturnVoid -> failure x
  SWhile exp stm -> failure x
  SDoWhile stm1 exp stm2 -> failure x
  SBlock stms -> failure x
  SIf exp stm -> failure x
  SIfElse exp stm1 stm2 -> failure x
  SFor decl exp1 exp2 stm -> failure x
  SType type_ id -> failure x
  SConst type_ exp -> failure x
  SStruct id decls -> failure x
transQId :: QId -> Result
transQId x = case x of
  QId id -> failure x
  QIdT id types -> failure x
transQConst :: QConst -> Result
transQConst x = case x of
  QConst qids -> failure x
transExp :: Exp -> Result
transExp x = case x of
  ETrue -> failure x
  EFalse -> failure x
  EInt integer -> failure x
  EDouble double -> failure x
  EChar char -> failure x
  EString strings -> failure x
  EQct qconst -> failure x
  EArr id exp -> failure x
  EApp id exps -> failure x
  EPEstr exp1 exp2 -> failure x
  EProy exp1 exp2 -> failure x
  EPIncr exp -> failure x
  EPDecr exp -> failure x
  EIncr exp -> failure x
  EDecr exp -> failure x
  ENeg exp -> failure x
  ETimes exp1 exp2 -> failure x
  EDiv exp1 exp2 -> failure x
  ERest exp1 exp2 -> failure x
  EPlus exp1 exp2 -> failure x
  EMinus exp1 exp2 -> failure x
  EShiftL exp1 exp2 -> failure x
  EShiftR exp1 exp2 -> failure x
  ELt exp1 exp2 -> failure x
  EGt exp1 exp2 -> failure x
  ELtEq exp1 exp2 -> failure x
  EGtEq exp1 exp2 -> failure x
  EEq exp1 exp2 -> failure x
  ENEq exp1 exp2 -> failure x
  EAnd exp1 exp2 -> failure x
  EOr exp1 exp2 -> failure x
  EMinAss exp1 exp2 -> failure x
  EAddAss exp1 exp2 -> failure x
  EAss exp1 exp2 -> failure x
  ECond exp1 exp2 exp3 -> failure x
  EExcp exp -> failure x
  ETyped exp type_ -> failure x
transBuilInType :: BuilInType -> Result
transBuilInType x = case x of
  BuilInType_bool -> failure x
  BuilInType_int -> failure x
  BuilInType_double -> failure x
  BuilInType_void -> failure x
transType :: Type -> Result
transType x = case x of
  TypeB builintype -> failure x
  TypeQ qconst -> failure x
  TypeP type_ -> failure x

